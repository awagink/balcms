<IfModule mod_rewrite.c> 
    RewriteEngine   on
    
    RewriteCond     %{REQUEST_FILENAME}     -f
    RewriteCond     %{REQUEST_URI}          \.css$
    RewriteCond     %{QUERY_STRING}         scaffold
    RewriteRule     ^(.+)$                  styles/scaffold.php?f=%{REQUEST_URI}&%{QUERY_STRING}
    
    RewriteCond     %{REQUEST_FILENAME}     -f
    RewriteCond     %{REQUEST_URI}          \.scss$
    RewriteRule     ^(.+)$                  styles/scaffold.php?f=%{REQUEST_URI}&%{QUERY_STRING}
</IfModule>